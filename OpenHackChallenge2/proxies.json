{
  "$schema": "http://json.schemastore.org/proxies",
  "proxies": {
    "GetProduct": {
      "matchCondition": {
        "methods": [ "GET" ],
        "route": "/api/GetProduct{*query}"
      },
      "backendUri": "https://serverlessohproduct.trafficmanager.net/api/GetProduct{query}"
    },
    "GetProducts": {
      "matchCondition": {
        "methods": [ "GET" ],
        "route": "/api/GetProducts"
      },
      "backendUri": "https://serverlessohproduct.trafficmanager.net/api/GetProducts"
    },
    "GetUser": {
      "matchCondition": {
        "methods": [ "GET" ],
        "route": "/api/GetUser{*query}"
      },
      "backendUri": "https://serverlessohuser.trafficmanager.net/api/GetUser{query}"
    }
  }
}